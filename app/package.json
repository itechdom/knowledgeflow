{
  "name": "celebrae",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf www/*",
    "assets": "cp -a ./index.html ./platforms/browser/www && cp -a ./assets/* ./platforms/browser/www",
    "start": "npm run prepare && cordova run android",
    "deploy": "npm run assets && ./node_modules/.bin/webpack --env.production && ./node_modules/.bin/serverless client deploy --no-confirm",
    "prepare": "npm run assets && npm run build && cordova prepare android && cordova prepare browser",
    "build": "./node_modules/.bin/webpack --env.production",
    "web": "rm -rf ./platforms/browser/www/index.js && ./node_modules/.bin/webpack-dev-server --mode=development --port 8085",
    "serve": "npm run web",
    "build:dev": "cp -a index.html ./www/ && ./node_modules/.bin/webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@markab.io/react": "^1.11.1",
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^4.3.3",
    "autosuggest-highlight": "^3.1.1",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-camera": "^4.1.0",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-media-capture": "^3.0.3",
    "formik": "^1.5.8",
    "google-map-react": "^1.1.5",
    "history": "4.9.0",
    "immutable": "^3.8.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-autosuggest": "^9.4.3",
    "react-big-calendar": "^0.22.0",
    "react-dom": "^16.7.0",
    "react-dropzone": "^10.1.10",
    "react-easy-swipe": "0.0.18",
    "react-force-graph-2d": "^1.12.0",
    "react-ga": "^2.7.0",
    "react-geolocated": "^3.0.1",
    "react-image-gallery": "^0.8.18",
    "react-inlinesvg": "^1.1.5",
    "react-loadable": "^5.5.0",
    "react-mde": "^7.6.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-simple-code-editor": "^0.10.0",
    "react-slick": "^0.24.0",
    "react-transition-group": "^4.3.0",
    "react-virtualized": "^9.21.2",
    "recharts": "^1.6.2",
    "recompose": "^0.30.0",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cordova-lib": "^9.0.1",
    "css-loader": "^1.0.1",
    "detect-port-alt": "^1.1.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^2.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "json-loader": "^0.5.4",
    "mobx-devtools-mst": "^0.9.18",
    "mobx-react-devtools": "^6.0.3",
    "ngrok": "^3.2.5",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "sass-loader": "^7.1.0",
    "serverless": "^1.47.0",
    "serverless-apig-s3": "^2.0.0",
    "serverless-finch": "^2.4.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.7.1",
    "cordova-plugin-whitelist": "1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-media-capture": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}
