            "22": {
              "title": "Tail Call Optimization",
              "id": "876.56e4f31a0711848f",
              "ideas": {
                "1": {
                  "title": "Resources",
                  "id": "879.56e4f31a0711848f",
                  "ideas": {
                    "1": {
                      "title": "http://2ality.com/2015/06/tail-call-optimization.html",
                      "id": "880.56e4f31a0711848f"
                    }
                  },
                  "attr": {
                    "collapsed": true
                  }
                },
                "0.5": {
                  "title": "Brain Storm",
                  "id": "885.56e4f31a0711848f",
                  "ideas": {
                    "1": {
                      "title": "There are functions that don't do much other than return the same value to the caller",
                      "id": "882.56e4f31a0711848f",
                      "ideas": {
                        "1": {
                          "title": "they can be optimized by being removing their stack frame",
                          "id": "883.56e4f31a0711848f"
                        }
                      },
                      "attr": {
                        "collapsed": true
                      }
                    },
                    "2": {
                      "title": "Or ",
                      "id": "884.56e4f31a0711848f",
                      "attr": {
                        "style": {
                          "width": 50
                        },
                        "collapsed": true
                      },
                      "ideas": {
                        "1": {
                          "title": "There is an intermediate function that is unnecessary in between said function and the last function",
                          "id": "886.56e4f31a0711848f",
                          "ideas": {
                            "1": {
                              "title": "so we remove its stack frame before calling the tail function and passing it to the original caller",
                              "id": "887.56e4f31a0711848f"
                            }
                          },
                          "attr": {
                            "collapsed": true
                          }
                        }
                      }
                    },
                    "3": {
                      "title": "the function has to be returned when called",
                      "id": "888.56e4f31a0711848f",
                      "ideas": {
                        "1": {
